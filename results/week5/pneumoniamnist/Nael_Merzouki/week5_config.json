{
  "week": 5,
  "dataset": "pneumoniamnist",
  "task": "linear_probing + domain_shift_robustness",
  "shared_core": {
    "label_fractions": [
      0.05,
      0.1
    ],
    "probe_epochs": 30,
    "probe_lr": 0.001,
    "probe_weight_decay": 0.0001,
    "encoder_dim": 512
  },
  "p3_domain_shift": {
    "description": "Mini domain shift experiments measuring AUROC/ECE change",
    "perturbations": {
      "gaussian_noise": {
        "std": 0.15
      },
      "brightness": {
        "delta": 0.2
      },
      "contrast": {
        "factor": 0.3
      }
    }
  },
  "seed": 42,
  "device": "cuda"
}